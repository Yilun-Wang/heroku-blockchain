<!DOCTYPE objects.html>
<html>

<head>
    <title>Prototype2</title>
    <meta charset="UTF-8">
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <link rel='stylesheet' href='/assets/css/main.css' />
</head>

<body>
    <!-- <h1>This webpage will show the interaction between Patient and MedDevices</h1> -->

    <!-- These three should be displayed in coloumns -->

    <style>
        #deviceView {
            float: left;
            text-align: center;
        }

        #eventLog {
            float: left;
            padding-left: 10px;
            max-width: 300px;
            text-align: center;
            font-size: 15px;
            overflow: auto;
        }

        #userView {
            float: right;
            text-align: center;
            padding-right: 50px;
            overflow: auto;
            background-image: url(/images/phone-frame.png);
            /* background-position: center; */
            background-size: 100% 100%;
            width: 400px;
            height: 600px;
        }

        
    </style>

    <div class="w3-row">
        <div class="w3-col m5 l5 w3-panel" id='deviceView'>
            <%- include device.ejs %>
        </div>

        <div class="w3-col m3 l3 w3-panel" id='eventLog'>
            <%- include eventLog.ejs %>
        </div>

        <div class="w3-col m4 l4 w3-panel" id='userView'>

            <%- include user.ejs %>
                
        </div>

    </div>


    <script type="text/javascript">
        $(document).ready(function () {

        });
    </script>

</body>

</html>